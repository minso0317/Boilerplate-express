  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "start": "node dist/src/server.js",
    "dev": "nodemon -r tsconfig-paths/register --watch src src/server.ts",
    "typecheck": "tsc --noEmit",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test-**test_file_name**-unit": "jest --selectProjects **test_file_name**-unit --runInBand --detectOpenHandles --coverage",
    "test-**test_file_name**-int": "jest --selectProjects **test_file_name**-integration --runInBand --detectOpenHandles --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^8.0.0",
    "express": "^5.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.13.0",
    "superstruct": "^2.0.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
